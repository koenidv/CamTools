    <string name="equation_hyperfocal">
        Hyperfocal distance \\(d_h\\):
        $$d_h = \\frac{f^2}{k + c} + f$$
        Near limit \\(d_n\\):
        $$d_n = \\frac{d_h^2}{2 * d_h - f}$$
    </string>
    <string name="equation_hyperfocal_description">
        f = Focal length\nk = Aperture\nc = Circle of confusion
    </string>
    <string name="equation_focuslimits">
        Hyperfocal distance \\(d_h\\):
        $$d_h = \\frac{f^2}{k + c} + f$$
        Near limit \\(d_n\\):
        $$d_n = \\frac{d_h * d}{d_h + d - f}$$
        Far limit \\(d_f\\):
        $$d_f = \\frac{d_h * d}{d_h - d - f}$$
    </string>
    <string name="equation_focuslimits_description">
        f = Focal length\nk = Aperture\nc = Circle of confusion\nd = Object distance
    </string>
    <string name="equation_reversefocus">
        Interim result \\(i\\):
        $$i = \\frac{ \\frac{f}{d_n} + \\frac{f}{d_f} - 2 }{ \\frac{1}{d_f} - \\frac{1}{d_n} }$$
        Focus distance \\(d\\):
        $$d = \\frac{ i - f }{ \\frac{i}{d_n} - 1 }$$
        Aperture \\(k\\):
        $$k = \\frac{f^2}{i * c}$$
    </string>
    <string name="equation_reversefocus_description">
        f = Focal length\ndn = Near limit\ndf = Far limit\nc = Circle of confusion
    </string>
    <string name="equation_nd">
        Long exposure \\(t_l\\):
        $$t_l = t_i * k^2$$
    </string>
    <string name="equation_nd_description">
        ti = Initial exposure time\nk = Filter strength (stops)
    </string>
    <string name="equation_spotstars">
        NPF rule \\(t_n\\):
        $$t_n = \\frac{ 35 * k + 30 * p }{ f }$$
        500 rule \\(t_5\\):
        $$t_5 = \\frac{ 500 }{ f }$$
        600 rule \\(t_6\\):
        $$t_6 = \\frac{ 600 }{ f }$$
    </string>
    <string name="equation_spotstars_description">
        k = Aperture\np = Pixel pitch\nf = Focal length
    </string>
    <string name="equation_cropfactor">
        Focal length \\(f\\):
        $$f = f_f * c$$
        Aperture \\(k\\):
        $$k = k_f * c$$
    </string>
    <string name="equation_cropfactor_description">
        ff = Full-frame focal length\nkf = Full-frame aperture\nc = Crop factor
    </string>

