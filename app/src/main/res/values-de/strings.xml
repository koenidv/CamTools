    <string name="equation_hyperfocal">
        Hyperfokale Distanz \\(d_h\\):
        $$d_h = \\frac{f^2}{k + z} + f$$
        Nahgrenze \\(d_n\\):
        $$d_n = \\frac{d_h^2}{2 * d_h - f}$$
    </string>
    <string name="equation_hyperfocal_description">
        f = Brennweite\nk = Blende\nz = Zerstreuungskreis
    </string>
    <string name="equation_focuslimits">
        Hyperfokale Distanz \\(d_h\\):
        $$d_h = \\frac{f^2}{k + z} + f$$
        Nahgrenze \\(d_n\\):
        $$d_n = \\frac{d_h * d}{d_h + d - f}$$
        Ferngrenze \\(d_f\\):
        $$d_f = \\frac{d_h * d}{d_h - d - f}$$
    </string>
    <string name="equation_focuslimits_description">
        f = Brennweite\nk = Blende\nz = Zerstreuungskreis\nd = Entfernung des Objekts
    </string>
    <string name="equation_reversefocus">
        Zwischenergebnis \\(i\\):
        $$i = \\frac{ \\frac{f}{d_n} + \\frac{f}{d_f} - 2 }{ \\frac{1}{d_f} - \\frac{1}{d_n} }$$
        Fokusdistanz \\(d\\):
        $$d = \\frac{ i - f }{ \\frac{i}{d_n} - 1 }$$
        Blende \\(k\\):
        $$k = \\frac{f^2}{i * z}$$
    </string>
    <string name="equation_reversefocus_description">
        f = Brennweite\ndn = Nahgrenze\ndf = Ferngrenze\nz = Zerstreuungskreis
    </string>
    <string name="equation_nd">
        Lange Belichtungszeit \\(t_l\\):
        $$t_l = t_n * k^2$$
    </string>
    <string name="equation_nd_description">
        tn = Normale Belichtungszeit\nk = Filterstärke (Stops)
    </string>
    <string name="equation_spotstars">
        NPF-Regel \\(t_n\\):
        $$t_n = \\frac{ 35 * k + 30 * p }{ f }$$
        500-Regel \\(t_5\\):
        $$t_5 = \\frac{ 500 }{ f }$$
        600-Regel \\(t_6\\):
        $$t_6 = \\frac{ 600 }{ f }$$
    </string>
    <string name="equation_spotstars_description">
        k = Blende\np = Pixelabstand\nf = Brennweite
    </string>
    <string name="equation_cropfactor">
        Brennweite \\(f\\):
        $$f = f_f * c$$
        Blende \\(k\\):
        $$k = k_f * c$$
    </string>
    <string name="equation_cropfactor_description">
        ff = Vollformat-Brennweite\nkf = Vollformat-Blende\nc = Formatfaktor
    </string>
